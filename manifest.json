{
  "manifest_version": 2,

  "name": "Facebook SRT Extension.",
  "description": "This extension provides shortcutKeys to improve productivity. [Ex: ALT+1/2/3...6]",
  "version": "1.0",
  "icons": { 
      "48": "facebook.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file:///*"],
      "js": ["shortcut.js"]
    }
  ],
  "background" : {
    "scripts": ["background.js"]
  },
  "commands": {
    "COMBINATION_1": {
      "suggested_key": {
        "default": "Alt+1"
      },
      "description": "COMBINATION_1"
    },
    "COMBINATION_2": {
      "suggested_key": {
        "default": "Alt+1"
      },
      "description": "COMBINATION_2"
    },
    "COMBINATION_3": {
      "suggested_key": {
        "default": "Alt+1"
      },
      "description": "COMBINATION_3"
    },
    "COMBINATION_4": {
      "suggested_key": {
        "default": "Alt+1"
      },
      "description": "COMBINATION_4"
    },
    "COMBINATION_5": {     
      "description": "COMBINATION_5"
    },
    "COMBINATION_6": {      
      "description": "COMBINATION_6"
    }
  }
}
